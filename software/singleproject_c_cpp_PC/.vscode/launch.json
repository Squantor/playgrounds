  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch pc example debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/debug/PC.elf",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build PC example debug configuration",
    },
    {
      "name": "(gdb) Launch pc example release",

      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/peripheral_abstraction/debug/peripheral_abstraction.elf",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
